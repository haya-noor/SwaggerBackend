{
  "name": "example-server-tsoa",
  "version": "1.0.0",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "tsoa": "^6.3.1",
    "uuid": "^10.0.0",
     "jsonwebtoken": "^8.5.1"
  },
  "scripts": {
    "start": "node ./build/bin/startServer.js",
    "dev": "ts-node ./bin/startServer.ts",
    "generate:swagger": "tsoa spec && tsoa routes",
    "upload:swagger": "ts-node scripts/uploadSwagger.ts",
    "build": "tsc && npm run generate:swagger && npm run upload:swagger",
    "prepare": "husky"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.0",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "yarn": "^1.22.22",
    "husky": "^8.0.3",
    "@types/jsonwebtoken": "^8.5.8"
  },
  "release": {
    "branches": ["main"],
    "repositoryUrl": "https://github.com/haya-noor/SwaggerBackend.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": ["package.json", "CHANGELOG.md"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
